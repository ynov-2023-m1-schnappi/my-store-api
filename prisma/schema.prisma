generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = "postgres://postgres:123456@localhost:5432/schnappi"
}

model Product {
  id          Int     @id @default(autoincrement())
  name        String
  description String?
  active      Boolean @default(false)
  thumbnail   String
  packshot    String  @default("/uploads/")
  price       Decimal @default(0.000000000000000000000000000000)
}
